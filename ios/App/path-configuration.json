{
  "settings": {
    "enable_feature_x": true
  },
  "rules": [
    {
      "patterns": [
        "/new$",
        "/edit$",
        "/signin$"
      ],
      "properties": {
        "context": "default",
        "pull_to_refresh_enabled": false
      }
    },
    {
      "patterns": [
        "^/employees/sign_in/$",
        "^/employees/sign_in$"
      ],
      "properties": {
        "title": "Northwind"
      }
    },
    {
      "patterns": [
        "/customers$"
      ],
      "properties": {
        "view_controller": "bar",
        "title": "Bar"
      }
    },
    {
      "patterns": [
        "/foo$"
      ],
      "properties": {
        "view_controller": "foo",
        "title": "Foo"
      }
    }
  ]
}
